{
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.2",
    "postcss-cli": "^11.0.0",
    "sass": "^1.84.0"
  },
  "scripts": {
    "compile": "sass --watch styles/scss/:styles/css/ --no-source-map --style=compressed --silence-deprecation=import",
    "prefix": "postcss --watch styles/css/*.css --no-map --use autoprefixer --dir styles/css/",
    "dev": "npm-run-all --parallel compile prefix",
    "build:compile": "sass styles/scss/:styles/css/ --no-source-map --style=compressed --silence-deprecation=import",
    "build:prefix": "postcss styles/css/*.css --no-map --use autoprefixer --dir styles/css/",
    "build": "npm-run-all build:compile build:prefix"
  }
}